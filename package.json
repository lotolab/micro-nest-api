{
  "name": "micro-nest-api",
  "version": "1.0.0",
  "description": "Nest API服务",
  "author": "lanbery",
  "private": true,
  "license": "LICENSED",
  "scripts": {
    "build": "nest build",
    "dev": "cross-env STAGE=dev nest start --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env STAGE=prod nest start",
    "start:debug": "cross-env STAGE=dev nest start --debug --watch",
    "start:prod": "cross-env STAGE=prod node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.6",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.8",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.8",
    "@supercharge/request-ip": "^1.2.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "chalk": "4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "eventsource-parser": "^2.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "iterare": "^1.2.1",
    "joi": "^17.13.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.7",
    "nanoid": "3.3.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "qs": "^6.12.1",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.0",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/node-fetch": "^2.6.11",
    "@types/passport-jwt": "^4.0.1",
    "@types/qs": "^6.9.15",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "colorette": "^2.0.20",
    "commander": "^12.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "express": "^4.19.2",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "yarn": "^1.22.22"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
